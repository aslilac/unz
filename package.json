{
	"name": "unzzz",
	"version": "0.9.0",
	"author": "McKayla Washburn",
	"description": "A lightweight zip archive reader",
	"license": "MIT",
	"files": [
		"target/*"
	],
	"main": "target/unzzz.js",
	"types": "target/types/unzzz.d.ts",
	"homepage": "https://unzzz.now.sh",
	"repository": "partheseas/unzzz",
	"bugs": "https://github.com/partheseas/unzzz/issues",
	"keywords": [
		"archive",
		"deflate",
		"inflate",
		"typescript",
		"zip"
	],
	"dependencies": {
		"gardens": "^4.2.0"
	},
	"devDependencies": {
		"@types/jest": "^26.0.19",
		"@types/node": "^14.14.14",
		"@typescript-eslint/eslint-plugin": "^4.9.1",
		"@typescript-eslint/parser": "^4.9.1",
		"ava": "^2.2.0",
		"eslint": "^7.15.0",
		"jest": "^26.6.3",
		"ts-jest": "^26.4.4",
		"typedoc": "^0.15.0",
		"typescript": "^4.1.3"
	},
	"scripts": {
		"build": "tsc -p .",
		"fmt": "prettier --ignore-file .gitignore --write .",
		"now-build": "typedoc",
		"test": "eslint --ext .js,.ts lib && rollup -c && ava",
		"type-check": "tsc -p . --noEmit"
	}
}
