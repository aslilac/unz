{
	"name": "unzzz",
	"version": "1.0.0",
	"author": "McKayla Washburn <mckayla@hey.com>",
	"description": "A ZIP archive reader built with native web APIs",
	"keywords": [
		"archive",
		"browser",
		"deflate",
		"inflate",
		"zip"
	],
	"license": "MIT",
	"homepage": "https://github.com/aslilac/unzzz",
	"repository": "https://github.com/aslilac/unzzz",
	"bugs": "https://github.com/aslilac/unzzz/issues",
	"type": "commonjs",
	"main": "./build/index.js",
	"module": "./build/module/index.js",
	"types": "./build/index.d.ts",
	"exports": {
		"require": "./build/index.js",
		"import": "./build/module/index.js"
	},
	"files": [
		"build/**"
	],
	"devDependencies": {
		"@mckayla/prettier": "^1.0.1",
		"@mckayla/tsconfig": "^1.0.2",
		"@types/jest": "^27.0.3",
		"@types/node": "^17.0.24",
		"eslint": "^8.13.0",
		"eslint-config-mckayla": "^1.0.4",
		"prettier": "^2.6.2",
		"jest": "^27.5.1",
		"ts-jest": "^27.1.4",
		"typedoc": "^0.22.15",
		"typescript": "^4.6.3"
	},
	"scripts": {
		"build": "npm run build:cjs -s && npm run build:esm -s",
		"build:cjs": "tsc -p . --module commonjs --declaration",
		"build:esm": "tsc -p . --module esnext --outdir ./build/module/",
		"dev": "tsc -p . -w",
		"fmt": "prettier --write .",
		"lint": "eslint .",
		"now-build": "typedoc",
		"prepack": "npm run build -s",
		"test": "jest",
		"type-check": "tsc -p . --noEmit"
	}
}
